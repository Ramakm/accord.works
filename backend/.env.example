# Backend environment example for Contract AI
# Copy this to backend/.env and fill real values

# Allowed frontend origin for CORS
FRONTEND_URL=http://localhost:3000

# Google Gemini
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.0-flash

# Dodo Payments (server-side)
# API key only needed if you call Dodo APIs from backend
DODO_API_KEY=
# Webhook signing secret for verifying Dodo webhooks
DODO_WEBHOOK_SECRET=

# Static payment link (Pro)
# Base domain for static checkout links
DODO_CHECKOUT_BASE=https://checkout.dodopayments.com/buy
# Your Pro product ID from Dodo dashboard (for static links)
DODO_PRO_PRODUCT_ID=pdt_twZ1kvnoPTcfg2F4yGSbI
# Default return/redirect URL after payment completion
DODO_RETURN_URL=http://localhost:3000/thank-you
